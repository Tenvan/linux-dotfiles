{
    "VARIABLES": [
        [
            "@@FILES_EDIT",
            "code -a --file-uri"
        ],
        [
            "@@FOLDER_EDIT",
            "code -a --folder-uri"
        ]
    ],
    "APP_MENU": [
        [
            "Bild Betrachter",
            "flatpak run org.photoqt.PhotoQt"
        ],
        [
            "🛟 JetBrains Toolbox",
            "jetbrains-toolbox"
        ],
        [
            "🙋 Teams",
            "flatpak run com.github.IsmaelMartinez.teams_for_linux"
        ],
        [
            "☯️ Virtio Manager",
            "virt-manager"
        ],
        [
            "⚪ Remmina",
            "remmina"
        ],
        [
            "🗃️ Dateimanager",
            "$FILEMANAGER"
        ],
        [
            "😃 Emoji Test",
            "$TERMINAL --hold curl https://unicode.org/Public/emoji/5.0/emoji-test.txt"
        ],
        [
            "🚸 UTF8 Test",
            "$TERMINAL --hold curl https://www.w3.org/2001/06/utf-8-test/UTF-8-demo.html"
        ],
        [
            "🌥️ Wetter Höxter",
            "$TERMINAL --hold --title Wetter:Höxter curl wttr.in/37671?lang=de"
        ]
    ],
    "DEVELOP_MENU": [
        [
            "🗂 Dateien WORKSPACE",
            "$FILEMANAGER $WORKSPACE"
        ]
    ],
    "SYSTEM_TOOLS_MENU": [
        [
            "⚙️ System Settings (LXQT)",
            "lxqt-config"
        ],
        [
            "🖥️ Monitor einrichten",
            "arandr"
        ],
        [
            "🪄 Grub Update (BTRFS Snapshots)",
            "$TERMINAL --hold --title Sys:Grubup sudo update-grub"
        ],
        [
            "⏰ Timeshift",
            "timeshift-launcher"
        ],
        [
            "📂 Belegung Verzeichnisse",
            "baobab"
        ],
        [
            "🎫 Erscheinungsbild (Lx)",
            "lxappearance"
        ],
        [
            "🎫 Erscheinungsbild (Xfce4)",
            "xfce4-appearance-settings"
        ],
        [
            "🎫 Erscheinungsbild (Qt5)",
            "qt5ct"
        ],
        [
            "📛 System Logs (user)",
            "gnome-logs"
        ],
        [
            "📛 System Logs (root)",
            "sudo gnome-logs"
        ],
        [
            "📛 awesome error log",
            "$TERMINAL --hold --title Sys:XSessionError multitail -cs -i $HOME/.xsession-stderr"
        ],
        [
            "📛 User Journal",
            "$TERMINAL --hold --title Sys:UserLog journalctl _UID=$(id --user) -f -n 1000"
        ],
        [
            "📛 Boot Log",
            "$TERMINAL --hold --title Sys:BootLog sudo multitail -cs -i /var/log/boot.log "
        ],
        [
            "📛 Installer Log",
            "$TERMINAL --hold --title Sys:InstallerLog sudo multitail -cs -i /var/log/dnf.log "
        ]
    ],
    "EDIT_CONFIG": [
        [
            "✏️ Editor",
            "code"
        ],
        [
            "📁 ansible dir",
            "@@FOLDER_EDIT $HOME/.ansible"
        ],
        [
            "📁 custom dir",
            "@@FOLDER_EDIT $HOME/.custom"
        ],
        [
            "📁 config dir",
            "@@FOLDER_EDIT $HOME/.config"
        ],
        [
            "📁 awesome dir",
            "@@FOLDER_EDIT $HOME/.config/awesome"
        ],
        [
            "📁 bin dir",
            "@@FOLDER_EDIT $HOME/.bin"
        ],
        [
            "📁 scripts dir",
            "@@FOLDER_EDIT $SCRIPTS"
        ],
        [
            "📁 menu dir",
            "@@FOLDER_EDIT $SCRIPTS/menu"
        ],
        [
            "📑 scripts install",
            "@@FILES_EDIT $SCRIPTS/install*"
        ],
        [
            "📑 shell configs",
            "@@FILES_EDIT $HOME/.xinit* $HOME/.alias* $HOME/.profile* $HOME/.xprofile* $HOME/.xsession* $HOME/.Xresources* $HOME/.zshrc* $HOME/.alias* $HOME/.bashrc* "
        ],
        [
            "📑 themes Files",
            "@@FILES_EDIT $HOME/.Xresources* $HOME/.gtkrc-*"
        ],
        [
            "📁 themes Folder",
            "@@FOLDER_EDIT  $HOME/.config/gtk-3.0 $HOME/.config/gtk-4.0 $HOME/.config/qt5ct $HOME/.config/fontconfig"
        ],
        [
            "📑 screenlayout",
            "@@FILES_EDIT $HOME/.screenlayout/*"
        ]
    ],
    "SYSTEM_MENU": [
        [
            "📲 Abmelden",
            "sh $SCRIPTS/session_logout.sh"
        ],
        [
            "📵 Sperren",
            "sh $SCRIPTS/session_lock.sh"
        ],
        [
            "📵 Bildschirm sperren",
            "sh $SCRIPTS/session_lock_screen.sh"
        ],
        [
            "🙅 Benutzerwechsel",
            "sh $SCRIPTS/session_switch_user.sh"
        ],
        [
            "🙋 Bereitschaft",
            "sh $SCRIPTS/session_suspend.sh"
        ],
        [
            "🙆 Hibernate",
            "sh $SCRIPTS/session_hibernate.sh"
        ],
        [
            "🪃 Neustart",
            "sh $SCRIPTS/session_reboot.sh"
        ],
        [
            "📴 Runterfahren",
            "sh $SCRIPTS/session_shutdown.sh"
        ]
    ],
    "SYSTEM_MONITOR": [
        [
            "📈 System Monitoring Center",
            "system-monitoring-center"
        ],
        [
            "📈 System Resourcen",
            "gnome-system-monitor -r"
        ],
        [
            "📈 bpytop",
            "$TERMINAL --title SysMon:bashtop bpytop"
        ],
        [
            "📈 glances",
            "$TERMINAL --title SysMon:glances glances"
        ],
        [
            "💽 iftop (sudo)",
            "$TERMINAL --title SysMon:iftop sudo iftop"
        ],
        [
            "💽 iotop (sudo)",
            "$TERMINAL --title SysMon:iotop sudo iotop -Pao"
        ],
        [
            "💽 iptraf-ng (sudo)",
            "$TERMINAL --title SysMon:iptraf-ng sudo iptraf-ng"
        ]
    ]
}